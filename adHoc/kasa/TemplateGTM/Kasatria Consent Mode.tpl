___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const dataLayerPush = require('createQueue')('dataLayer');
const gtag = require('createArgumentsQueue')('gtag', 'dataLayer');
const gtagSet = require('gtagSet');
const log = require('logToConsole');

const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');

// Set advanced settings
gtagSet({
  url_passthrough: data.url_passthrough || false,
  ads_data_redaction: data.ads_data_redaction || false
});

const splitInput = (input) => {
  return input.split(',')
    .map(entry => entry.trim())
    .filter(entry => entry.length !== 0);
};

// dataLayer.push helper
const dlPush = (isDefault, ads, analytics, personalization, functionality, security, region) => {
  dataLayerPush({
    event: 'gtm_consent_' + (isDefault ? 'default' : 'update'),
    ad_storage: ads,
    analytics_storage: analytics,
    personalization_storage: personalization,
    functionality_storage: functionality,
    security_storage: security,
    consent_region: region
  });
};

const parseDefaultData = (settings) => {
  const regions = splitInput(settings.regions);

  const commandData = {};
  if(settings.wait_for_update > 0) {
    commandData.wait_for_update = settings.wait_for_update;
  }

  if (regions.length > 0 && regions !== 'all' ) {
    commandData.region = regions;
  }
  if(settings.ad_storage === 'granted' || settings.ad_storage === 'denied') {
    commandData.ad_storage = settings.ad_storage;
  }
  if(settings.analytics_storage === 'granted' || settings.analytics_storage === 'denied') {
    commandData.analytics_storage = settings.analytics_storage;
  }
  if(settings.personalization_storage === 'granted' || settings.personalization_storage === 'denied') {
    commandData.personalization_storage = settings.personalization_storage;

  if(settings.functionality_storage === 'granted' || settings.functionality_storage === 'denied') {
    commandData.functionality_storage = settings.functionality_storage;
  }
  }
  if(settings.security_storage === 'granted' || settings.security_storage === 'denied') {
    commandData.security_storage = settings.security_storage;
  }

  return commandData;
};

// Process default consent state

if(data.Command === 'default') {
  data.default_settings.forEach(settings => {
      const commandData = parseDefaultData(settings);
      setDefaultConsentState(commandData);
      if (data.sendGtag) gtag('consent', 'default', commandData);

      if (data.sendDataLayer){

      dlPush(true, settings.ad_storage, settings.analytics_storage, settings.personalization_storage, settings.functionality_storage, settings.security_storage, commandData.region);
    }
  });
}

// Process updated consent state
if (data.Command === 'update') {

  const commandData = {};
  if(data.update_ad_storage === 'granted' || data.update_ad_storage === 'denied') {
    commandData.ad_storage = data.update_ad_storage;
  }
  if(data.update_analytics_storage === 'granted' || data.update_analytics_storage === 'denied') {
    commandData.analytics_storage = data.update_analytics_storage;
  }
  if(data.update_personalization_storage === 'granted' || data.update_personalization_storage === 'denied') {
    commandData.personalization_storage = data.update_personalization_storage;
  }
  if(data.update_functionality_storage === 'granted' || data.update_functionality_storage === 'denied') {
    commandData.functionality_storage = data.update_functionality_storage;
  }
  if(data.update_security_storage === 'granted' || data.update_security_storage === 'denied') {
    commandData.security_storage = data.update_security_storage;
  }
  updateConsentState(commandData);

  if (data.sendGtag) {
    gtag('consent', 'update', {
      ad_storage: commandData.update_ad_storage,
      analytics_storage: commandData.update_analytics_storage,
      personalization_storage: commandData.update_personalization_storage,
      functionality_storage: commandData.update_functionality_storage,
      security_storage: commandData.update_security_storage
    });
  }
  if (data.sendDataLayer) {
    dlPush(false, data.update_ad_storage, data.update_analytics_storage, data.update_personalization_storage, data.update_functionality_storage, data.update_security_storage);
  }
}

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


